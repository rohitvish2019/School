        <div class="center-body">
          <div class="quick-links-section">
            
                <a class="btn btn-success" href="/registration/new">Registration</a>
                <a class="btn btn-success" href="/student/showByClass/fee">Fees</a>
                <a class="btn btn-success" href="/student/showByClass/result">Result</a>
